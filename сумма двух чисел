#include <iostream>
using namespace std;
int main()
{
     int a,b; // Объявление двух целочисленных переменных a и b
    
    // Вывод сообщения с просьбой ввести два целых числа
    cout <<"Введите 2 целых числа" << endl;
    
    // Ввод двух чисел от пользователя и сохранение их в переменные a и b
    cin >> a >> b;
    
    // Вывод результата сложения двух введенных чисел
    cout << "Их сумма равна" << a + b;
    
    return 0 ; // Завершение программы с возвратом кода 0 (успешное выполнение)
}
