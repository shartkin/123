#include<iostream>  
using namespace std; 

int main()
{
    // Объявление и инициализация переменных
    int  a  = 100 , b;    // a - целое число 100, b - неинициализированная целая переменная
    float c = 4.5 , d;    // c - вещественное число 4.5, d - неинициализированная вещественная переменная
    
    d = a/c;  // Деление целого числа на вещественное, результат сохраняется в вещественную переменную d
    cout << "d= " << d << endl; // Вывод значения d
    
    b = a/c;  // Деление целого числа на вещественное, результат сохраняется в целую переменную b
    cout << "b= " << b << endl; // Вывод значения b
    
    return 0 ; // Завершение программы
}
