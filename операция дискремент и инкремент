#include <iostream>  // Подключение библиотеки для ввода/вывода
using namespace std; // Использование стандартного пространства имен

int main()
{
    int x = 3, y = 4; // Инициализация переменных: x = 3, y = 4
    
    // Префиксные инкремент и декремент
    cout << ++x << "\t" << --y << endl; // Сначала изменяем, потом выводим
    
    // Постфиксные инкремент и декремент  
    cout << x++ << "\t" << y-- << endl; // Сначала выводим, потом изменяем
    
    // Вывод конечных значений переменных
    cout << x << "\t" << y << endl;
    
    return 0; // Завершение программы
}
