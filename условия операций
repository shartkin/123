#include<iostream>
using namespace std;
int main()
{
    int x, y, max;  // Объявление переменных для двух чисел и максимума
    
    cin >> x >> y;  // Ввод двух чисел с клавиатуры
    
    // Первый способ: непосредственный вывод большего числа с помощью тернарного оператора
    (x > y) ? cout << x : cout << y << endl;
    
    // Второй способ: сохранение результата в переменную max с помощью тернарного оператора
    max = (x > y) ? x : y;
    cout << max << endl;  // Вывод значения переменной max
    
    return 0;
}
