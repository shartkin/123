#include<iostream>
using namespace std;

int main()
{
    int a, b;
    cout << "введите a и b: ";  // Исправлена опечатка
    cin >> a;
    cout << a << " ";           // Добавлен пробел для читаемости
    cin >> b;
    cout << b << endl;          // Добавлен перевод строки
    
    // Вычисление начального значения (ближайшее нечетное число >= a)
    int i = (a % 2) ? a : a + 1;  // Исправлен тернарный оператор
    
    while(i <= b){
        cout << i * i << " ";  // Вывод квадрата нечетного числа
        i += 2;                // Увеличиваем на 2 для следующего нечетного числа
    }
    return 0;
}
\\int a, b;
    cout << "введите a и b: ";
    cin >> a >> b;
    
    // Цикл for для нечетных чисел от a до b
    for(int i = (a % 2) ? a : a + 1; i <= b; i += 2) {
        cout << i * i << " ";
    } \\
