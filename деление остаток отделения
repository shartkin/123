#include<iostream>
using namespace std;
int main()
{
    // Целочисленное деление - дробная часть отбрасывается
    cout << 100/24 << "\t" << 100/24 << endl;
    
    // Смешанное деление - целое на целое и вещественное на целое
    cout << 100/21 << "\t" << 100.0/24 << endl;
    
    // Операция взятия остатка от деления
    cout << 21%3 << "\t" << 21%6 << "\t" << -21%8 << endl;
    
    return 0;
}
