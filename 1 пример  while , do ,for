#include<iostream>

using namespace std;

int main()
{
    int a,b; // Объявление переменных a и b
    cout << "ввкдите a и b"; // Вывод приглашения для ввода (опечатка: должно быть "введите")
    cin>>a; // Ввод значения a
    cin>>b; // Ввод значения b

    int i = 0; // Инициализация счетчика i нулем

    // Бесконечный цикл! Условие a <= b не изменяется внутри цикла
    while(a <= b)
    cout << i * i++ <<"\t"; // Вывод произведения i на i++ с табуляцией

    return 0; // Завершение программы (эта строка может никогда не выполниться из-за бесконечного цикла)
}
// Закомментированная альтернативная реализация с циклом for:
// for(int i=a; i<=b; i++)  // Цикл от a до b включительно
//     cout << i * i <<endl; // Вывод квадрата каждого числа i
