#include<iostream>
using namespace std;
int main()
{
  int x; // Объявление переменной для хранения номера дня недели
  cin >> x; // Ввод номера дня недели от пользователя
  cout <<"введите будни или выходные";
  
  switch (x) // Проверка значения переменной x
  {
     // Дни с 1 по 5 - рабочие дни
     case 1: case 2: case 3: case 4:
     case 5: cout << "рабочий день"; break; // Вывод "рабочий день" и выход из switch
     
     // Дни 6 и 7 - выходные дни
     case 6: case 7: cout << "выходной"; break; // Вывод "выходной" и выход из switch
     // ОШИБКА: отсутствует точка с запятой после break (добавлена)
     
     // Любое другое значение - ошибка ввода
     default: cout << "вы ошиблись"; // Вывод сообщения об ошибке
  }
    return 0;
}
